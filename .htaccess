# Turn Rewrite Engine On
RewriteEngine on

RewriteRule ^page/([0-9a-zA-Z]+) index.php?page=$1 [NC,L]

RewriteRule ^category/([0-9a-zA-Z]+) index.php?category=$1 [NC,L]

RewriteRule ^([0-9a-zA-Z]+)/([0-9a-zA-Z_]+) index.php?page=$1&userid=$2 [NC,L]

RewriteRule ^([0-9a-zA-Z]+)/([0-9a-zA-Z_]+)/([0-9a-zA-Z]+) index.php?page=$1&userid=$2&category=$3 [NC,L]
